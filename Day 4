def find_sum_of_list(lst):
    # Using the built-in sum() function
    total_sum = sum(lst)
    return total_sum

# Get user input for a list of numbers
user_input = input("Enter a list of numbers separated by spaces or commas: ")

# Convert the input string into a list of integers
user_list = [int(num) for num in user_input.replace(',', ' ').split()]

# Call the function with the user's list
result = find_sum_of_list(user_list)

# Display the result
print(f"The sum of the elements in the list is: {result}")

