# Input your own list of numbers separated by space or comma
input_numbers = input("Enter a list of numbers separated by spaces or commas: ")

# Replace commas with spaces and then split the input string into a list of integers
input_numbers = input_numbers.replace(",", " ")
numbers = [int(x) for x in input_numbers.split()]

# Use a lambda function to square each element
squared_numbers = list(map(lambda x: x ** 2, numbers))

# Print the squared numbers
print("Squared numbers:", squared_numbers)
